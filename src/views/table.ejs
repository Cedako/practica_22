<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="assets/style.css" type="text/css" rel="stylesheet">
    <title>Tabla de personas</title>
</head>
<body>
    <table>
        <tr>
            <th>SSN</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Age</th>
            <th>Blood</th>
        </tr>
        <% Object.keys(persons).forEach(function(prop) { %>
        <tr>
            <td> <%= persons[prop].socialServiceNumber %> </td>
            <td> <%= persons[prop].firstName %> </td>
            <td> <%= persons[prop].lastName %> </td>
            <td> <%= persons[prop].age %> </td>
            <td> <%= persons[prop].bloodType %> </td>
            <td> <a href="<%= `deletePerson/${persons[prop]._id}` %>">Delete</a> </td>
            <td> <a href="<%= `editPerson/${persons[prop]._id}` %>">Edit</a> </td>
        </tr>
        <% }); %>
    </table>
    <a href="./index">Back</a> <br>

    <h1>D i s e Ã± o</h1>
</body>
</html>